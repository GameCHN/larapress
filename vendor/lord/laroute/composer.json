    "keywords"  : ["laravel", "routes", "routing", "javascript"],
    "authors": [
        {
            "name": "Aaron Lord",
            "email": "hello@aaronlord.is"
        }
    ],
    "license" : "MIT",
    "require": {
        "php" : ">=5.4.0",
        "illuminate/support" : "5.0.*|5.1.*",
        "illuminate/routing": "5.0.*|5.1.*",
        "illuminate/console": "5.0.*|5.1.*",
        "illuminate/config": "5.0.*|5.1.*",
        "illuminate/filesystem": "5.0.*|5.1.*"
    },
    "require-dev" : {
        "phpunit/phpunit" : "~4.0",
        "mockery/mockery" : "dev-master"
    },
    "autoload": {

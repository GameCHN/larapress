    <meta name="renderer" content="webkit"><?php /* 360浏览器使用webkit内核渲染页面 */ ?>
    <meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1" /><?php /* IE(内核)浏览器优先使用高版本内核 */ ?>


    <?php echo $__env->yieldSection(); ?><?php /* 在内联样式之前填充一些东西 */ ?>

    <?php $__env->startSection('head_style'); ?>
    <?php echo $__env->yieldSection(); ?><?php /* head区域内联css样式表 */ ?>

